{
  "name": "nuxt-nitro-module-kit",
  "version": "0.0.4",
  "description": "Nitro module kit to develop modules for both Nuxt/Nitro with Nuxt Kit experience.",
  "keywords": [
    "nuxt",
    "nitro",
    "nuxt",
    "module"
  ],
  "homepage": "https://github.com/wuiyang/nuxt-nitro-module-kit#readme",
  "bugs": {
    "url": "https://github.com/wuiyang/nuxt-nitro-module-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wuiyang/nuxt-nitro-module-kit.git"
  },
  "license": "MIT",
  "author": "Tan Wui Yang",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.mjs",
      "default": "./dist/utils.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev:prepare": "unbuild --stub",
    "dev:nitro": "nitro dev --dir examples/nitro",
    "dev:nuxt": "nuxt dev --cwd examples/nuxt",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "prepack": "npm run build",
    "release": "npm run test && npm run build && changelogen --release && npm publish && git push --follow-tags",
    "test": "npm run lint && npm run test:types",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@nuxt/kit": "^3.16.0",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@types/node": "^20.19.9",
    "changelogen": "^0.6.2",
    "eslint": "^9.32.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "globals": "^16.3.0",
    "nitropack": "^2.12.4",
    "nuxt": "^3.16.0",
    "typescript-eslint": "^8.38.0",
    "unbuild": "^3.6.0"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "errx": "^0.1.0",
    "mlly": "^1.8.0",
    "pathe": "^2.0.3",
    "unctx": "^2.4.1",
    "untyped": "^2.0.0"
  }
}
